{
  "name": "avtonom/geocoder-bundle",
  "type": "symfony-bundle",
  "description": "Geocoder Sypex-geo bundle for Symfony2 bundle for PHP",
  "keywords": ["Symfony2", "bundle", "geocoder", "geo", "sypex", "sypex-geo", "sypexGeo"],
  "homepage": "https://github.com/Avtonom/geocoder-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton U",
      "email": "avtonomspb@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/dependency-injection": "~2.3|3.*",
    "willdurand/geocoder-bundle": "@stable",
    "yamilovs/sypex-geo-bundle": "~1.2"
  },
  "autoload": {
    "psr-0": { "Avtonom\\GeocoderBundle": "" }
  },
  "target-dir": "Avtonom/GeocoderBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  }
}
